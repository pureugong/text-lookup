{
    "manifest_version": 3,
    "name": "ScaleTone",
    "version": "1.0",
    "permissions": [
      "contextMenus",
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "web_accessible_resources": [{
      "resources": ["data.csv"],
      "matches": ["<all_urls>"]
    }]
  }
  